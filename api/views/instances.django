<p>Your instances</p>

<div class="row">
  {% for item in instances %}
    <div class="col-4 mb-3">
      <div class="card">
        <div class="card-body">
          <h5 class="card-text">{{ item.Name }}</h5>
          <p class="card-title">{{ item.Description }}</p>
          <p class="card-title">Instance ID: {{ item.ID }}</p>
        </div>
      </div>
    </div>
  {% endfor %}
</div>
